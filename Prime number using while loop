#prime number or not
Number=int(input('enter number:'))
i=1
Count=0
while (i<=Number//2):
        if(Number%i==0):
             Count+=1
        i+=1
if (count==1):
         print("%d is a prime number" %(Number))
else:
         print("%d is not a prime number" %(Number))




